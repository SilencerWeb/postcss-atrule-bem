{
  "name": "postcss-atrule-bem",
  "description": "Transpile to BEM Components",
  "version": "3.0.0",
  "main": "dist/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "repository": {
    "url": "https://github.com/tbremer/postcss-bem-atrule.git",
    "type": "git"
  },
  "author": "Tom Bremer <tom@tbremer.com>",
  "license": "MIT",
  "scripts": {
    "build:es": "NODE_ENV=es babel src/ --out-dir es/",
    "build:commonjs": "NODE_ENV=commonjs babel src/ --out-dir dist/",
    "build": "yarn build:es && yarn build:commonjs",
    "prepublish": "yarn build",
    "lint": "eslint ./",
    "test": "mocha --compilers js:babel-register",
    "test:watch": "yarn test -- --watch --reporter min"
  },
  "keywords": [
    "postcss",
    "postcss plugin",
    "bem",
    "atrule",
    "css"
  ],
  "dependencies": {
    "postcss": "^6.0.8"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.24.1",
    "eslint": "^4.3.0",
    "eslint-config-tbremer": "^1.2.1",
    "expect": "^1.20.2",
    "mocha": "^3.5.0",
    "pre-commit": "^1.1.3"
  },
  "precommit": [
    "lint"
  ]
}
